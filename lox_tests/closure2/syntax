FunctionDeclaration {
    name: "outer",
    body: Block {
        stmt: Var {
            name: "x",
            initializer: Some(
                StringLiteral(value),
            ),
        },
        stmt: FunctionDeclaration {
            name: "middle",
            body: Block {
                stmt: FunctionDeclaration {
                    name: "inner",
                    body: Block {
                        stmt: Print {
                            expr: Variable(x),
                        },
                    },
                },
                stmt: Expr {
                    expr: Call {
                        callee: Variable(inner),
                    },
                },
            },
        },
        stmt: Expr {
            expr: Call {
                callee: Variable(middle),
            },
        },
    },
}
Expr {
    expr: Call {
        callee: Variable(outer),
    },
}
