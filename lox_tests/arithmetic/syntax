    expr: BinaryOp {
        left: NumberLiteral(1),
        op: Plus,
        right: BinaryOp {
            left: NumberLiteral(2),
            op: Star,
            right: NumberLiteral(3),
        },
    },
}
Expr {
    expr: BinaryOp {
        left: NumberLiteral(1),
        op: Minus,
        right: BinaryOp {
            left: NumberLiteral(2),
            op: Slash,
            right: NumberLiteral(3),
        },
    },
}
Expr {
    expr: BinaryOp {
        left: NumberLiteral(1),
        op: Plus,
        right: BinaryOp {
            left: NumberLiteral(2),
            op: Star,
            right: UnaryOp {
                op: Minus,
                expr: NumberLiteral(3),
            },
        },
    },
}
